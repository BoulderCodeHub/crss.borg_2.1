    RULE_EXEC_CONSTRAINT @"t" >= @"24:00:00 January Max DayOfMonth, Current Year" AND @"t" <= @"24:00:00 July Max DayOfMonth, Current Year" AND NOT "HasRuleFiredSuccessfully"( "ThisRule" );
    NOTES                "";
