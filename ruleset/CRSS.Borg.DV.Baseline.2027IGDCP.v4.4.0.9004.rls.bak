    END
    UUID "{17136ba4-c64c-4c2b-939c-1a90c22ab2fd}";;

    RULE                 "Set Unset Outflow";
    DESCRIPTION          "Sets the outflow for spillCalc and power methods, registering dependencies so that higher priority rules can set values. ";
